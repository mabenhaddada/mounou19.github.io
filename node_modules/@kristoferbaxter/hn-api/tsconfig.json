{
  "compilerOptions": {
    "outDir": "./lib/",
    "sourceMap": false,
    "module": "esnext",
    "moduleResolution": "node",
    "target": "es2017",
    "baseUrl": ".",
    "paths": {
      "storage/*": [
        "src/storage/*"
      ]
    },
    "allowJs": false,
    "noUnusedLocals": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src/**/*"
  ]
}